        {
          "_name": "people02",
          "_sourceTheme": "preschoolm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"people02\" group=\"People\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Screen\" name=\"fullScreen\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"fullScreen == false\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <header>Row</header>\n        <select title=\"Align\" name=\"contentAlign\">\n            <option value=\"justify-content-start\">Left</option>\n            <option value=\"justify-content-center\" selected>Center</option>\n            <option value=\"justify-content-end\">Right</option>\n        </select>\n        <header>Card</header>\n        <input type=\"range\" inline title=\"Amount\" name=\"cardAmount\" min=\"1\" max=\"6\" step=\"1\" value=\"6\">\n        <input type=\"checkbox\" title=\"Image\" name=\"showImage\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Transparent\" name=\"isTransparent\" checked>\n        <input type=\"color\" title=\"Color\" name=\"cardColor\" value=\"#ffffff\" condition=\"!isTransparent\">\n        <input type=\"checkbox\" title=\"Shadow\" name=\"hasShadow\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/preschoolm5/components/_images/bg/4-bg.jpg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/preschoolm5/components/_images/bg/1-bg.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"wrapper\">\n            <div class=\"row {{contentAlign}}\">\n                <div class=\"col-12 title-section\" mbr-if=\"showTitle\">\n                    <h2 class=\"mbr-section-title mbr-fonts-style\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\" data-app-placeholder=\"Type Text\">\n                        <b>Let Us Introduce Our Staff</b>\n                    </h2>\n                </div>\n                <div class=\"col-12 col-lg-4 card-padding\">\n                    <div class=\"card m-0\">\n                        <div class=\"card-body p-5\">\n                            <div class=\"img-container mb-3\" mbr-if=\"showImage\">\n                                <img src=\"@PROJECT_PATH@/assets/images/21-460x460.png\" alt class=\"card-img-top\">\n                            </div>\n                            <h3 class=\"card-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Armiyaâ€™u Ahmed Yelwa </b><i>BMLS , PDE, DCS, MSc</i>&nbsp;&nbsp;</h3>\n                            <h4 class=\"mbr-section-subtitle title2 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title2\" data-app-placeholder=\"Type Text\" mbr-if=\"showSubtitle\">Managing Director</h4>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg-4 card-padding\" mbr-if=\"cardAmount > 1\">\n                    <div class=\"card m-0\">\n                        <div class=\"card-body p-5\">\n                            <div class=\"img-container mb-3\" mbr-if=\"showImage\">\n                                <img src=\"@PROJECT_PATH@/assets/images/3-460x460.png\" alt class=\"card-img-top\">\n                            </div>\n                            <h3 class=\"card-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>&nbsp;Abdulqahar Abdullahi, Esq&nbsp;</b></h3>\n                            <h4 class=\"mbr-section-subtitle title2 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title2\" data-app-placeholder=\"Type Text\" mbr-if=\"showSubtitle\">Secretary/Legal Adviser</h4>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg-4 card-padding\" mbr-if=\"cardAmount > 2\">\n                    <div class=\"card m-0\">\n                        <div class=\"card-body p-5\">\n                            <div class=\"img-container mb-3\" mbr-if=\"showImage\">\n                                <img src=\"@PROJECT_PATH@/assets/images/4-460x460.png\" alt class=\"card-img-top\">\n                            </div>\n                            <h3 class=\"card-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Abdulmumin Ahmed&nbsp;</b></h3>\n                            <h4 class=\"mbr-section-subtitle title2 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title2\" data-app-placeholder=\"Type Text\" mbr-if=\"showSubtitle\">Director Operations</h4>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg-4 card-padding\" mbr-if=\"cardAmount > 3\">\n                    <div class=\"card m-0\">\n                        <div class=\"card-body p-5\">\n                            <div class=\"img-container mb-3\" mbr-if=\"showImage\">\n                                <img src=\"@PROJECT_PATH@/assets/images/5-460x460.png\" alt class=\"card-img-top\">\n                            </div>\n                            <h3 class=\"card-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Abdulkabir Ahmed</b></h3>\n                            <h4 class=\"mbr-section-subtitle title2 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title2\" data-app-placeholder=\"Type Text\" mbr-if=\"showSubtitle\">Director Technical Support</h4>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg-4 card-padding\" mbr-if=\"cardAmount > 4\">\n                    <div class=\"card m-0\">\n                        <div class=\"card-body p-5\">\n                            <div class=\"img-container mb-3\" mbr-if=\"showImage\">\n                                <img src=\"@PROJECT_PATH@/assets/images/6-460x460.png\" alt class=\"card-img-top\">\n                            </div>\n                            <h3 class=\"card-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Sadiya Yusuf</b></h3>\n                            <h4 class=\"mbr-section-subtitle title2 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title2\" data-app-placeholder=\"Type Text\" mbr-if=\"showSubtitle\">Director Business Development</h4>\n                        </div>\n                    </div>\n                </div>\n                <div class=\"col-12 col-lg-4 card-padding\" mbr-if=\"cardAmount > 5\">\n                    <div class=\"card m-0\">\n                        <div class=\"card-body p-5\">\n                            <div class=\"img-container mb-3\" mbr-if=\"showImage\">\n                                <img src=\"@PROJECT_PATH@/assets/images/7-460x460.png\" alt class=\"card-img-top\">\n                            </div>\n                            <h3 class=\"card-title mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".card-title\" data-app-placeholder=\"Type Text\" mbr-if=\"showCardTitle\"><b>Zakiyya Hafiz Ashana&nbsp;</b></h3>\n                            <h4 class=\"mbr-section-subtitle title2 mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".title2\" data-app-placeholder=\"Type Text\" mbr-if=\"showSubtitle\">Director Finance and sales</h4>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "& when not (@fullScreen)": {
              "padding-top": "(@paddingTop * 1rem)",
              "padding-bottom": "(@paddingBottom * 1rem)"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".card": {
              "height": "auto",
              "background-color": "if(@isTransparent, transparent, @cardColor)",
              "& when (@hasShadow)": {
                "box-shadow": "0px 10px 20px 0px rgba(29, 29, 27, 0.2)"
              },
              "transition": "all 0.6s ease",
              "&:hover": {
                "transform": "scale(1.03)"
              }
