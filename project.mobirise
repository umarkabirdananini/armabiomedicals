              }
            },
            ".img-container": {
              "display": "flex",
              "justify-content": "center"
            },
            ".card-img-top": {
              "width": "230px",
              "height": "230px",
              "border-radius": "50%",
              "object-fit": "cover"
            },
            ".mbr-section-subtitle": {
              "text-align": "center"
            },
            ".mbr-text": {
              "text-align": "center"
            },
            ".mbr-fallback-image.disabled": {
